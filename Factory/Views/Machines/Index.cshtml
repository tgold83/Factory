@{
  Layout = "_Layout";
}

@using Factory.Models;
@model List<Factory.Models.Machine>;

<h1>Machines</h1>

<ul>
  @foreach(Machine machine in Model)
  {
    <li>@Html.ActionLink($"{machine.Title}", "Details", new { id = machine.MachineId } )</li>
  }
</ul>

<p>@Html.ActionLink("Add a new machine", "Create")</p>
<p>@Html.ActionLink("Home", "Index", "Home")</p>